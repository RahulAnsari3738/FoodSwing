{
  "ConnectionStrings": {
    "FoodSwing": "Data Source=(local);Initial Catalog=FoodSwingDBApp;Integrated Security=True;Connect Timeout=60;Encrypt=False;"
  },


  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Serilog": {

    "MinimumLevel": "Information",

    "Override": {

      "Microsoft.AspNetCore": "Warning"

    },

    "WriteTo": [

      {

        "Name": "Console"

      },

      {

        "Name": "File",

        "Args": {

          "path": "logs\\AppLogs.log"

        }

      },

      {

        "Name": "File",

        "Args": {

          "path": "logs\\AppJSONLogs.log",

          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"

        }

      }

    ]


}}


  

